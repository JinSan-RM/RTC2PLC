<TimeBox@BoxLayout>
    orientation: 'horizontal'
    title: "취출 SOL"

    Label:
        text: root.title

    Button:
        text: "0.0 초"
        background_normal: ""
        background_down: ""
        background_color: 0, 0, 0, 0
        color: 1, 1, 1, 1
        on_press: root.on_time_setting(self, "delay")

    Button:
        text: "0.0 초"
        background_normal: ""
        background_down: ""
        background_color: 0, 0, 0, 0
        color: 1, 1, 1, 1
        on_press: root.on_time_setting(self, "duration")